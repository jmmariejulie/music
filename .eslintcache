  case 1: return '16'; // double croche\r\n        }\r\n    }\r\n    return \"\";\r\n}\r\n\r\nconst getInstruments = (noteSequence: NoteSequence): number[] => {\r\n    let instruments: number[] = [];\r\n    for (var i = 0; i < noteSequence.notes.length; i++) {\r\n        const note = noteSequence.notes[i];\r\n        if (!instruments.includes(note.instrument!)) {\r\n            instruments.push(note.instrument!);\r\n        }\r\n    }\r\n    return instruments;\r\n}\r\n\r\nconst copyToSequence = (sourceSequence: NoteSequence, destSequence: NoteSequence, instrument: number): void => {\r\n    for (var i = 0; i < sourceSequence.notes.length; i++) {\r\n        const note = sourceSequence.notes[i];\r\n        if (note.instrument === instrument) {\r\n            destSequence.notes.push(note);\r\n        }\r\n    }\r\n}\r\n\r\n",["61","62"],"C:\\development\\workspace\\music\\src\\components\\MultiStaffVisualizer.tsx",["63","64"],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":35,"column":13,"nodeType":"71","messageId":"72","endLine":35,"endColumn":20},{"ruleId":"69","severity":1,"message":"73","line":94,"column":15,"nodeType":"71","messageId":"72","endLine":94,"endColumn":35},{"ruleId":"69","severity":1,"message":"73","line":65,"column":15,"nodeType":"71","messageId":"72","endLine":65,"endColumn":35},{"ruleId":"69","severity":1,"message":"74","line":5,"column":23,"nodeType":"71","messageId":"72","endLine":5,"endColumn":33},{"ruleId":"69","severity":1,"message":"75","line":5,"column":35,"nodeType":"71","messageId":"72","endLine":5,"endColumn":46},{"ruleId":"69","severity":1,"message":"76","line":7,"column":39,"nodeType":"71","messageId":"72","endLine":7,"endColumn":47},{"ruleId":"77","severity":1,"message":"78","line":28,"column":26,"nodeType":"79","messageId":"80","endLine":28,"endColumn":28},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"81","severity":1,"message":"76","